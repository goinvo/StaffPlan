<div class='grid-row staffing-row'>
  <div class="name-or-client"><a href="/clients/{{client_id}}">{{name}}</a></div>
  <div class="flex staffing-work-weeks">
    {{#each weekly_totals}}
      <div class="staffing-work-week" data-toggle="popover" data-content="info about the users assigned to the client's projects here">
        {{total_hours}}
      </div>
    {{/each}}
  </div>
</div>
