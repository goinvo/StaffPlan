<div class='grid-row staffing-row'>
  <div class="name-or-client"><a href="/staffplans/{{user_id}}">{{first_name}} {{last_name}}</a></div>
  <div class="flex staffing-work-weeks">
    {{#each weekly_totals}}
      <div class="staffing-work-week" data-toggle="popover" data-content="info about the user's client projects here">
        {{total_hours}}
      </div>
    {{/each}}
  </div>
</div>
